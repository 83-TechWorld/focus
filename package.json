{
  "name": "focus",
  "displayName": "productive-foucs",
  "description": "This extension will help you focus on your work and achieve your goals within a specific timeframe",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
      "command": "productive-focus.startFocus",
      "title": "Focus-Start"
    },
    {
      "command": "productive-focus.extendFocus",
      "title": "I can extend the focus."
    },
    {
      "command": "productive-focus.closeFocus",
      "title": "Goal Achieved - Close"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
